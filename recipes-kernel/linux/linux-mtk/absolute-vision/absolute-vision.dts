&pmic {
	interrupt-parent = <&pio>;
	interrupts = <222 IRQ_TYPE_LEVEL_HIGH>;

	pmic_key: pmic-key {
		compatible = "mediatek,mt6359-keys";
		mediatek,long-press-mode = <1>;
		power-off-time-sec = <0>;

		power-key{
			linux,keycodes = <116>;
			wakeup-source;
		};

		home-key{
			linux,keycodes = <114>;
		};
	};
};

&scp {
	memory-region = <&scp_mem>;
	status = "okay";
	firmware-name = "mediatek/mt8188/scp.img";
};

&uart0 {
	pinctrl-0 = <&uart0_pins>;
	pinctrl-names = "default";
	status = "okay";
};

&uart1 {
	pinctrl-0 = <&uart1_pins>;
	pinctrl-names = "default";
	status = "okay";
};

&uart2 {
	pinctrl-0 = <&uart2_pins>;
	pinctrl-names = "default";
	status = "okay";
};

&mipi_tx0 {
	status = "disabled";
};


&spmi {
	#address-cells = <2>;
	#size-cells = <0>;

	mt6315_6: mt6315@6 {
		compatible = "mediatek,mt6315-regulator";
		reg = <0x6 SPMI_USID>;

		regulators {
			mt6315_6_vbuck1: vbuck1 {
				regulator-compatible = "vbuck1";
				regulator-name = "Vbcpu";
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1193750>;
				regulator-enable-ramp-delay = <256>;
				regulator-allowed-modes = <0 1 2>;
				regulator-always-on;
				mtk,combined-regulator = <2>;
			};
			mt6315_6_vbuck3: vbuck3 {
				regulator-compatible = "vbuck3";
				regulator-name = "Vdd2";
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1193750>;
				regulator-enable-ramp-delay = <256>;
				regulator-allowed-modes = <0 1 2>;
				regulator-always-on;
			};
			mt6315_6_vbuck4: vbuck4 {
				regulator-compatible = "vbuck4";
				regulator-name = "Vddq";
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1193750>;
				regulator-enable-ramp-delay = <256>;
				regulator-allowed-modes = <0 1 2>;
				regulator-always-on;
			};
		};
	};
};

&u3phy0 {
	status = "okay";
};

&u3phy1 {
	status = "okay";
};

&u3phy2 {
	status = "okay";
};

&ssusb0 {
	pinctrl-0 = <&usb_default>;
	pinctrl-names = "default";
	maximum-speed = "high-speed";
	usb-role-switch;
	dr_mode = "host";
	vusb33-supply = <&mt6359_vusb_ldo_reg>;
	status = "okay";
	wakeup-source;
};

&u2port0 {
	status = "okay";
};

&xhci0 {
	status = "okay";
	vusb33-supply = <&mt6359_vusb_ldo_reg>;
};

&ssusb1 {
	pinctrl-0 = <&usb1_default>;
	pinctrl-names = "default";
	vusb33-supply = <&mt6359_vusb_ldo_reg>;
	dr_mode = "host";
	status = "okay";
	wakeup-source;

	connector {
		compatible = "gpio-usb-a-connector", "usb-a-connector";
		type = "micro";
		id-gpios = <&pio 86 GPIO_ACTIVE_HIGH>;
		vbus-supply = <&usb_p1_vbus>;
	};
};

&u2port1 {
	status = "okay";
};

&u3port1 {
	status = "okay";
};

&xhci1 {
	status = "okay";
	vusb33-supply = <&mt6359_vusb_ldo_reg>;
};

&ssusb2 {
	pinctrl-0 = <&usb2_default>;
	pinctrl-names = "default";
	maximum-speed = "high-speed";
	usb-role-switch;
	dr_mode = "otg";
	vusb33-supply = <&mt6359_vusb_ldo_reg>;
	status = "okay";
	wakeup-source;

	connector {
		compatible = "gpio-usb-b-connector", "usb-b-connector";
		type = "micro";
		id-gpios = <&pio 89 GPIO_ACTIVE_HIGH>;
		vbus-supply = <&usb_p2_vbus>;
	};
};

&u2port2 {
	status = "okay";
};

&xhci2 {
	status = "okay";
	vusb33-supply = <&mt6359_vusb_ldo_reg>;
};

&cam_vcore {
	domain-supply = <&mt6359_vproc1_buck_reg>;
};

&img_vcore {
	domain-supply = <&mt6359_vproc1_buck_reg>;
};

&accdet {
	mediatek,eint-level-pol = <IRQ_TYPE_LEVEL_LOW>;
};

&afe {
	#sound-dai-cells = <0>;
	memory-region = <&afe_dma_mem_reserved>;
	mediatek,dmic-iir-on;
	mediatek,dmic-clk-mono;
	mediatek,etdm-out2-multi-pin-mode;
	status = "okay";
};

&adsp {
	memory-region = <&adsp_dma_mem_reserved>,
		<&adsp_mem_reserved>;
};

&sound {
	compatible = "mediatek,mt8188-mt6359";
	model = "mt8370-evk";
	pinctrl-names = "default";
	pinctrl-0 = <&aud_pins_default>;
	audio-routing =
		"Headphone", "Headphone L",
		"Headphone", "Headphone R",
		"AIN1", "Headset Mic",
		"DMIC_INPUT", "AP DMIC",
		"AP DMIC", "AUDGLB",
		"AP DMIC", "MIC_BIAS_0",
		"AP DMIC", "MIC_BIAS_2";
	status = "okay";

	dai-link-0 {
		sound-dai = <&afe>;
		link-name = "DL_SRC_BE";

		codec {
			sound-dai = <&pmic 0>, <&accdet>;
		};
	};

	dai-link-1 {
		sound-dai = <&afe>;
		link-name = "UL_SRC_BE";

		codec {
			sound-dai = <&pmic 0>, <&amic_codec>;
		};
	};

	dai-link-2 {
		sound-dai = <&afe>;
		link-name = "DMIC_BE";

		codec {
			sound-dai = <&dmic_codec>;
		};
	};

	dai-link-3 {
		sound-dai = <&afe>;
		link-name = "ETDM3_OUT_BE";
		status = "disabled";

		codec {
			sound-dai = <&hdmi0>;
		};
	};

	dai-link-4 {
		sound-dai = <&afe>;
	        link-name = "ETDM2_OUT_BE";
		status = "okay";

		codec {
			sound-dai = <&tlv320aic310x>;
		};
	};
};
