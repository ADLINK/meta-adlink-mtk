@@ -495,6 +495,10 @@
 		"done; "                                                  \
 		"setenv devplist\0"					  \
 	\
+	"interfacetest=usb reset; echo ;usb tree;echo ;net list;echo ;"     \
+	"i2c bus; echo ;gpio status -a;echo ;setenv ipaddr 192.168.1.111 ;" \
+	"setenv serverip 192.168.1.1; ping $serverip\0"        		    \
